<%- include('partials/header') %>


  <div class="game-board">

    <li>
      <a href="/game/addName" class="link-menu">Ajoutez votre nom</a>
    </li>
  
    <p class="score">Votre score est de <%=score%>/10</p>

    <div class="result">
      <% gameArray.forEach(elem => { %>
       <p><%=elem.name%> - <%=elem.capital%> - 
        <% if (elem.capital.toLowerCase()===elem.user) { %>
          <span class="ok"><%=elem.user%></span></p>
          <% } else {%>
            <span class="ko"><%=elem.user%></span></p>
         <%}%>
       <br>
      <% }) %> 
    </div>

    <a href="/" class="link-game">Rejouer</a>


  </div>

<%- include('partials/footer') %>